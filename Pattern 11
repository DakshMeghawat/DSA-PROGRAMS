#include <iostream>
using namespace std;
int main()
{
    int start=0,n=5;
    for(int i=0;i<n;i++)
    {
        if (i%2==0)
        {
            start=1;
        }
        else if(i%2!=0){
            start=0;
        }
        for(int j=0;j<=i;j++)
        {
            cout<<start;
            start=1-start;
        }
        cout<<"\n";
        
        
    }
}
