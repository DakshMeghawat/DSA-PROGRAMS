#include<stdio.h>
#include<iostream>
#include<conio.h>
using namespace std;
int main()
{
    
    //printTriangle(5);
    int n=5;
    for (int i=1;i<=n;i++)
        {
            for (int j=1;j<=i;j++)
            {
                cout<<i<<" ";
            }
            cout<<"\n";
        }
    return 0;    
}
