
#include <iostream>

using namespace std;

int main()
{
    int n=5;
    for(int j=1;j<=n;j++)
        {
            for (int i=1;i<=n;i++)
            {
                if(j==1 || j==n)
                {
                    cout<<"*";
                
                }
                else if(i==1 || i==n){
                    cout<<"*";
                    
                }
                else{
                    cout<<" ";
                }
               
            }
            cout<<"\n";
        }

    return 0;
}
